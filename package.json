{
  "name": "@fxp/satis-serverless",
  "version": "1.0.0",
  "description": "Serverless Composer repository for private PHP packages hosted on Github",
  "main": "src/lambda.js",
  "repository": "git@github.com:fxpio/fxp-satis-serverless.git",
  "author": "Fran√ßois Pluchino <francois.pluchino@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "config": "node bin/config",
    "deconfig": "node bin/deconfig",
    "serve": "node bin/serve",
    "create-bucket": "node bin/create-bucket",
    "delete-bucket": "node bin/delete-bucket",
    "build": "node bin/build",
    "package": "node bin/package",
    "deploy": "node bin/deploy",
    "package-deploy": "node bin/package-deploy",
    "delete-stack": "node bin/delete-stack",
    "setup": "yarn install && node bin/setup",
