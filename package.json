{
  "name": "@fxp/satis-serverless",
  "version": "1.0.0",
  "description": "Serverless Composer repository for private PHP packages",
  "main": "src/lambda.js",
  "repository": "git@github.com:fxpio/fxp-satis-serverless.git",
  "author": "Fran√ßois Pluchino <francois.pluchino@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "v": "node -p \"require('./package.json').version\"",
    "config": "node bin/config",
    "deconfig": "node bin/deconfig",
    "serve": "node bin/serve",
    "create-bucket": "node bin/create-bucket",
    "delete-bucket": "node bin/delete-bucket",
    "build": "node bin/build",
    "package": "node bin/package",
    "deploy": "node bin/deploy",
    "package-deploy": "node bin/package-deploy",
    "delete-stack": "node bin/delete-stack",
    "setup": "yarn install && node bin/setup",
    "test": "ava --verbose",
    "test:watch": "ava --watch",
    "create-github-oauth": "node bin/create-github-oauth",
    "show-github-oauth": "node bin/show-github-oauth",
    "delete-github-oauth": "node bin/delete-github-oauth",
    "create-github-token": "node bin/create-github-token",
    "show-github-token": "node bin/show-github-token",
    "delete-github-token": "node bin/delete-github-token",
    "create-api-key": "node bin/create-api-key",
    "delete-api-key": "node bin/delete-api-key",
    "enable-repo": "node bin/enable-repo",
    "disable-repo": "node bin/disable-repo",
    "delete-packages": "node bin/delete-packages",
    "refresh-packages": "node bin/refresh-packages",
    "refresh-cache": "node bin/refresh-cache"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register",
      "tsconfig-paths/register"
    ]
  },
  "dependencies": {
    "aws-serverless-express": "awslabs/aws-serverless-express#content-length-fix",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "basic-auth": "^2.0.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.2",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "joi": "^13.4.0",
    "lodash.merge": "^4.6.1",
    "material-design-icons-iconfont": "^3.0.3",
    "node-fetch": "^2.1.2 <2.2.0",
    "node-polyglot": "^2.3.0",
    "vee-validate": "^2.1.0-beta.6",
    "vue": "^2.5.16",
    "vue-class-component": "^6.2.0",
    "vue-meta": "^1.5.2",
    "vue-property-decorator": "^7.0.0",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.16",
    "vue-template-loader": "^1.0.0",
    "vuetify": "^1.1.7",
    "vuex": "^3.0.1",
    "vuex-i18n": "^1.10.5"
  },
  "devDependencies": {
    "@types/aws-serverless-express": "^3.0.1",
    "@types/basic-auth": "^1.1.2",
    "@types/chai": "^4.1.4",
    "@types/compression": "^0.0.36",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^4.0.3",
    "@types/fs-extra": "^5.0.4",
    "@types/joi": "^13.4.0",
    "@types/lodash.merge": "^4.6.3",
    "@types/mocha": "^5.2.5",
    "@types/node-fetch": "^2.1.1",
    "@types/node-polyglot": "^0.4.31",
    "@types/vuex-i18n": "^1.7.1",
    "archiver": "^2.1.1",
    "async": "^2.6.1",
    "ava": "^1.0.0-beta.6",
    "aws-sdk": "^2.263.1",
    "commander": "^2.15.1",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "dotenv": "^6.0.0",
    "file-loader": "^1.1.11",
    "fs-extra": "^6.0.1",
    "ini": "^1.3.5",
    "inquirer": "^6.0.0",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.9.2",
    "nodemon": "^1.17.5",
    "nodemon-webpack-plugin": "^4.0.3",
    "offline-plugin": "^5.0.5",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "sass-loader": "^7.0.3",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.0",
    "tsconfig-paths": "^3.5.0",
    "typescript": "^2.9.2",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "unzip": "^0.1.11",
    "webpack": "^4.12.2",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.5",
    "webpack-manifest-plugin": "^2.0.3",
    "webpack-pwa-manifest": "^3.6.3",
    "yargs": "^11.1.0"
  }
}
